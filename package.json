{
  "name": "steady-sprouts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "google-map-react": "^0.25.0",
    "google-maps-react": "^1.1.0",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-google-maps": "^9.2.1",
    "react-materialize": "^1.0.16",
    "react-scripts": "^3.4.2",
    "serve": "^6.3.1"
  },
  "devDependencies": {
    "react-scripts": "3.4.2"
  },
  "scripts": {
    "build-css": "node-sass src/styles/sass/ -o src/styles/generated-sources/",
    "watch-css": "npm run build-css --watch --recursive",
